() fleet.fc

// Структура корабля
struct Ship {
    int name;
    int level;
}

// Хранение флота
storage {
    Ship[] fleet;
}

// Добавление корабля
void add_ship(int name, int level) impure {
    fleet.push(Ship(name, level));
}

// Получение флота
Ship[] get_fleet() method_id {
    return fleet;
}